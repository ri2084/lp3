#include <bits/stdc++.h>
using namespace std;

// Function to solve the 0/1 Knapsack problem using Dynamic Programming
int knapsack(int capacity, const vector<int>& weights, const vector<int>& values, int n) {
    // Create a DP table with dimensions (n+1) x (capacity+1)
    vector<vector<int>> dp(n + 1, vector<int>(capacity + 1, 0));

    // Build the DP table in a bottom-up manner
    for (int i = 1; i <= n; i++) { // Loop through items
        for (int w = 1; w <= capacity; w++) { // Loop through capacities
            if (weights[i - 1] <= w) {
                // Include or exclude the current item
                dp[i][w] = max(
                    dp[i - 1][w],                               // Exclude item i
                    values[i - 1] + dp[i - 1][w - weights[i - 1]] // Include item i
                );
            } else {
                // If weight exceeds current capacity, exclude it
                dp[i][w] = dp[i - 1][w];
            }
        }
    }

    // The last cell contains the maximum value for full capacity
    return dp[n][capacity];
}

int main() {
    int n, capacity;

    // Input number of items
    cout << "Enter number of items: ";
    cin >> n;

    vector<int> weights(n), values(n);

    // Input weights
    cout << "Enter weights of items: ";
    for (int i = 0; i < n; i++) {
        cin >> weights[i];
    }

    // Input values
    cout << "Enter values of items: ";
    for (int i = 0; i < n; i++) {
        cin >> values[i];
    }

    // Input maximum capacity
    cout << "Enter knapsack capacity: ";
    cin >> capacity;

    // Compute maximum possible value
    int max_value = knapsack(capacity, weights, values, n);

    cout << "\nThe maximum value that can be obtained is: " << max_value << endl;

    return 0;
}


// int n = 4; int capacity = 9; vector<int> weights = {2, 3, 4, 5}; vector<int> values = {3, 4, 5, 6};

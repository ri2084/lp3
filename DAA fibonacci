#include <iostream>
using namespace std;


int fibonacci_iterative(int n) {
    if (n == 0) return 0;
    if (n == 1) return 1;

    int prev2 = 0, prev1 = 1, current;
    for (int i = 2; i <= n; i++) {
        current = prev1 + prev2;
        prev2 = prev1;
        prev1 = current;
    }
    return current;
}

int fibonacci_recursive(int n) {
    if (n == 0) return 0;
    if (n == 1) return 1;
    return fibonacci_recursive(n - 1) + fibonacci_recursive(n - 2);
}

int main() {
    int n, choice;
    cout << "Enter the value of n: ";
    cin >> n;

     

    int result1 = fibonacci_iterative(n);
    cout << "Fibonacci(" << n << ") = " << result1 << " [Iterative]" << endl;

    int result2 = fibonacci_recursive(n);
    cout << "Fibonacci(" << n << ") = " << result2 << " [Recursive]" << endl;

    return 0;
}
